{
  "vale-off": {
    "prefix": "vale-off",
    "scope": "markdown,mdx",
    "body": ["<!-- vale off -->"],
    "description": "vale-off"
  },
  "vale-on": {
    "prefix": "vale-on",
    "scope": "markdown,mdx",
    "body": ["<!-- vale on -->"],
    "description": "vale-on"
  },
  "md-link-off": {
    "prefix": "md-link-off",
    "scope": "markdown.mdx",
    "body": [
      "<!-- markdown-link-check-disable -->"
    ],
    "description": "Disable Link Check"
  },
  "md-link-on": {
    "prefix": "md-link-on",
    "scope": "markdown,mdx",
    "body": [
      "<!-- markdown-link-check-enable -->"
    ],
    "description": "Enable Link Check"
  },
  "md-lint-disable": {
    "prefix": "md-lint-disable",
    "scope": "markdown,mdx",
    "body": [
      "<!-- markdownlint-disable -->"
    ],
    "description": "Disable Markdown Lint"
  },
  "md-lint-enable": {
    "prefix": "md-lint-enable",
    "scope": "markdown,mdx",
    "body": [
      "<!-- markdownlint-enable -->"
    ],
    "description": "Enable Markdown Lint"
  },
  "alex-disable": {
    "prefix": "alex-disable",
    "scope": "markdown,mdx",
    "body": [
      "<!--alex disable ${1:word}-->"
    ],
    "description": "alex-disable"
  },
  "alex-enable": {
    "prefix": "alex-enable",
    "scope": "markdown,mdx",
    "body": [
      "<!--alex enable ${1:word}-->"
    ],
    "description": "alex-enable"
  },
  "cpell-disable": {
    "prefix": "cpell-disable",
    "scope": "markdown,mdx",
    "body": [
      "<!-- cSpell:disable -->"
    ],
    "description": "cpell-disable"
  },
  "cpell-enable": {
    "prefix": "cpell-enable",
    "scope": "markdown,mdx",
    "body": [
      "<!-- cSpell:enable -->"
    ],
    "description": "cpell-enable"
  }
}
